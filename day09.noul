whole_input := read_file(
    __file__.path_parent
    path_join "inputs"
    path_join "input_day09.txt"
);

tail_visited := [[0] ** 100] ** 100;
tail_pos := [0, 0];
head_pos := [0, 0];

for (line <- lines whole_input) (
    dir, amt := words line;
    switch (dir)
        case "U" -> (print "uwu")
        case "D" -> (print "ddddd")
        case "L" -> (print "Lefto")
        case "R" -> (print "rightoo")
)
